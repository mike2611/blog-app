<%= render partial: "shared/user_container", locals: { user: @user } %>
<div class="container border">
  <h2>Bio</h2>
  <%= @user.bio%>
</div>
<% @posts.each do |post| %>
<%= render partial: "shared/post_container", locals: { post: post} %>
<% end %>
<div class="d-flex justify-content-center">
  <button> See all posts </button>
</div>
