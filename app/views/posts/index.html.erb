<%= link_to 'Logout', users_sign_out_path %>

<%= render partial: "shared/user_container", locals: { user: @user } %>
<% @posts.each do |post| %>
  <%= render partial: "shared/post_container", locals: { post: post} %>
  <%= render partial: "shared/comments_container", locals: { post: post} %>
<% end %>
